	<scriptlet name="idempiere" class="com.cenol.jasper.Scriptlet"/>
	<parameter name="RECORD_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select * from (
SELECT img.binarydata as logo,og.name organization_name ,oi.taxid  ,iv.documentno invoiceno ,iv.dateinvoiced , pt.name paymentterm , adddays(iv.dateinvoiced ,pt.netdays) datepayment , so.datepromised dateship ,
       oil.address1 ,oil.address2 , oil.address3 , oil.address4 ,oil.city , oil.regionname , oil.postal ,oi.phone , oi.fax ,
       bp.name as customer_name ,coalesce(iv.poreference,so.poreference) as poreference ,so.documentno as sales_order_no ,
       loc.address1 invoice_address1,loc.address2 invoice_address2, loc.address3 invoice_address3, loc.address4 invoice_address4 ,
       loc.city invoice_city,loc.regionname invoice_regionname, loc.postal invoice_postal,bpl.phone customer_phone, bpl.fax customer_fax ,
       shpl.address1 ship_address1,shpl.address2 ship_address2, shpl.address3 ship_address3, shpl.address4 ship_address4 ,
       shpl.city ship_city,shpl.regionname ship_regionname, shpl.postal ship_postal ,
       docl.address1 doc_address1,docl.address2 doc_address2, docl.address3 doc_address3, docl.address4 doc_address4 ,
       docl.city doc_city,docl.regionname doc_regionname, docl.postal doc_postal , ivt.taxamt , ivt.taxbaseamt , iv.grandtotal, iv.description remark , t.rate ,u.description as bill_issuerer
       ---- Detail ----------
       ,ivl.line
       ,pd.name product_name
       ,pd.value   product_code
       ,uom.name uom
       ,ivl.qtyentered
       ,ivl.linenetamt
       ,ivl.pricelist - ivl.priceentered discount_amt
       ,ivl.priceentered
       ,ivl.pricelist
       ,ivl.priceactual
       ,ivl.description as line_description
       ,dt.documentnote
       ,bu.name as customer_contractor
from c_invoice iv
left join c_order so on so.c_order_id = iv.c_order_id
inner join c_paymentterm pt on iv.c_paymentterm_id = pt.c_paymentterm_id
inner join ad_org og on og.ad_org_id = iv.ad_org_id
inner join ad_orginfo oi on oi.ad_org_id = og.ad_org_id
left join ad_image img on img.ad_image_id = oi.logo_id
left join c_location oil on oil.c_location_id = oi.c_location_id
inner join c_bpartner bp on bp.c_bpartner_id = iv.c_bpartner_id
inner join c_bpartner_location bpl on bpl.c_bpartner_location_id = iv.c_bpartner_location_id
inner join c_location loc on bpl.c_location_id = loc.c_location_id
inner join c_invoiceline ivl on iv.c_invoice_id = ivl.c_invoice_id AND ivl.isdescription = 'N'
left join m_product pd on pd.m_product_id = ivl.m_product_id
left join c_uom uom on uom.c_uom_id = ivl.c_uom_id
left join c_invoicetax ivt on iv.c_invoice_id = ivt.c_invoice_id
left join c_tax t on ivt.c_tax_id = t.c_tax_id
left join c_bpartner_location ship on ship.isshipto = 'Y' AND bp.c_Bpartner_Id = ship.c_bpartner_id
left join c_location shpl on ship.c_location_id = shpl.c_location_id
left join c_bpartner_location doc on doc.ispayfrom = 'Y' AND bp.c_bpartner_id = doc.c_bpartner_id
left join c_location docl on docl.c_location_id = doc.c_location_id
left join ad_user u on u.ad_user_id = iv.salesrep_id
left join ad_user bu on bu.ad_user_id = iv.ad_user_id
inner join c_doctype dt on dt.c_doctype_id = iv.c_doctypetarget_id
WHERE iv.DOCSTATUS <> 'VO'
AND dt.DOCBASETYPE = 'ARI'
AND iv.C_Invoice_ID =  $P{RECORD_ID} 
) t
order by t.invoiceno , t.line]]>
