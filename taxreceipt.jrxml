	<scriptlet name="idempiere" class="com.cenol.jasper.Scriptlet"/>
	<parameter name="RECORD_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT rpt.DOCUMENTNO receiptNo , rpt.datetrx as receipt_date , rpt.description as remark
	,og.name as organization_name ,ogi.TAXID , img.BINARYDATA as logo
	,ogl.ADDRESS1  ,ogl.ADDRESS2 , ogl.ADDRESS3  ,ogl.ADDRESS4  ,ogl.CITY ,ogl.regionname,ogl.POSTAL
	,ogi.PHONE , ogi.PHONE2 , ogi.FAX 
	, bp.NAME as Customer_Name,l.ADDRESS1 customer_address1,l.ADDRESS2 customer_address2,l.ADDRESS3 customer_address3,l.ADDRESS4 customer_address4,l.CITY customer_city,l.regionname customer_regionname,l.POSTAL customer_postal , bp.taxid as cust_tax_id
	,bpl.PHONE as CUSTOMER_TEL , bpl.PHONE2 as CUSTOMER_TEL2 , bpl.FAX as CUSTOMER_FAX
	, inv.DOCUMENTNO as invoiceNo , ivl.line , ivl.qtyentered , ivl.priceentered , ivl.linenetamt   , uom.NAME as UOM
	, pd.value as product_code , pd.name as product_name , ivl.description as line_description, COALESCE(ivt.TAXAMT,0) as TAXAMT , COALESCE(ivt.TAXBASEAMT,inv.TOTALLINES) as TAXBASEAMT ,inv.totallines , inv.grandtotal ,COALESCE(t.rate,0) as rate
	, rpt.payamt , rpt.discountamt , coalesce(rpt.WriteOffAmt,0) as withholdingamt
	, rpt.tendertype tendertype  , rpt.checkno , rpt.accountno , rpt.a_name , rpt.micr , rpt.routingno
	, rpt.creditcardtype , rpt.creditcardnumber
       ,bu.name as customer_contractor 
FROM C_PAYMENT rpt
INNER JOIN AD_ORG og ON og.AD_ORG_ID = rpt.AD_ORG_ID
INNER JOIN AD_ORGINFO ogi ON og.AD_ORG_ID = ogi.AD_ORG_ID
INNER JOIN C_LOCATION ogl ON ogl.C_LOCATION_ID = ogi.C_LOCATION_ID
LEFT JOIN AD_IMAGE img ON img.AD_IMAGE_ID = ogi.LOGO_ID
INNER JOIN C_INVOICE inv ON inv.C_INVOICE_ID = rpt.C_INVOICE_ID
INNER JOIN C_INVOICELINE ivl ON ivl.C_INVOICE_ID = inv.C_INVOICE_ID
LEFT JOIN M_PRODUCT pd ON ivl.M_PRODUCT_ID = pd.M_PRODUCT_ID
LEFT JOIN C_INVOICETAX ivt ON ivt.C_INVOICE_ID = inv.C_INVOICE_ID
LEFT JOIN C_TAX t ON ivt.C_TAX_ID = t.C_TAX_ID
INNER JOIN C_UOM uom ON uom.C_UOM_ID = ivl.C_UOM_ID
INNER JOIN C_BPARTNER bp ON bp.C_BPARTNER_ID = rpt.C_BPARTNER_ID
INNER JOIN C_BPARTNER_LOCATION bpl ON bp.C_BPARTNER_ID = bpl.C_BPARTNER_ID
INNER JOIN C_LOCATION l ON bpl.C_LOCATION_ID = l.C_LOCATION_ID
left join ad_user bu on bu.ad_user_id = inv.ad_user_id
WHERE rpt.isReceipt = 'Y'
AND rpt.C_Payment_ID =  $P{RECORD_ID} 
AND (ivl.M_PRODUCT_ID >0 OR ivl.C_CHARGE_ID >0)
AND inv.ispaid = 'Y'
ORDER BY rpt.documentno , ivl.line]]>
	</queryString>
