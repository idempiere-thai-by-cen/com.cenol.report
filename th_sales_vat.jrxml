	<queryString>
		<![CDATA[select o.name as orgname , oi.ishq , oi.branchno , oi.taxid as org_taxid ,oi.phone ,oi.phone2 , oi.fax , 
	  l.address1 ,l.address2, l.address3,l.address4 ,l.city ,l.regionname , l.postal , p.name as period_name ,
	  dt.name as doctype , t.name as taxname , vat.taxinvoiceno ,vat.taxinvoicedate , vat.bpname as bp_name , 
	  vat.taxid  as bp_taxid , vat.isHq as bp_ishq , vat.branchno as bp_branchno , vat.taxbaseamt , vat.taxamt ,
	  case when dt.docbasetype = 'APC' then -1 else 1 end as docbase_sign , vat.note 
from th_vattrx vat
inner join ad_org o on coalesce(vat.ad_reportingorg_id ,vat.ad_org_id) = o.ad_org_id 
inner join ad_orginfo oi on oi.ad_org_id = o.ad_org_id
inner join c_location l on oi.c_location_id = l.c_location_id
inner join c_doctype dt on dt.c_doctype_id = vat.c_doctype_id
inner join c_tax t on vat.c_tax_id = t.c_tax_id
inner join c_period p on vat.taxinvoicedate <= p.enddate and vat.taxinvoicedate >= p.startdate
where vat.issotrx = 'Y'
and vat.ad_client_id =  $P{AD_Client_ID} 
and coalesce(vat.ad_reportingorg_id ,vat.ad_org_id) =  $P{AD_ReportingOrg_ID} 
and vat.c_bpartner_id =  coalesce($P{C_BPartner_ID}   , vat.c_bpartner_id ) 
and vat.taxinvoiceno <= coalesce($P{DocumentNo1} , vat.taxinvoiceno)
and vat.taxinvoiceno >= coalesce($P{DocumentNo2} , vat.taxinvoiceno)
and vat.taxinvoicedate <= coalesce( $P{DateInvoiced1}  , vat.taxinvoicedate)
and vat.taxinvoicedate >= coalesce( $P{DateInvoiced2} , vat.taxinvoicedate)
order by p.PeriodNo  , dt.name ,  t.name , vat.taxinvoicedate , vat.taxinvoiceno]]>
